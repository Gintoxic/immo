server <- function(input, output) {
set.seed(122)
histdata <- rnorm(500)
output$plot1 <- renderPlot({
data <- histdata[seq_len(input$slider)]
if (input$checkb==F)
{
hist(data)
}else{hist(data, col="red")}
})
}
shinyApp(ui, server)
